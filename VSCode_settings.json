{
  "ruby.intellisense": "rubyLocate",
  "editor.formatOnType": true,
  "editor.minimap.enabled": false,
  "editor.find.seedSearchStringFromSelection": true,
  "editor.fontFamily": "Operator Mono, Source Code Pro, Menlo, 'Apple Color Emoji'",
  "editor.fontSize": 13,
  "editor.formatOnPaste": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.rulers": [120],
  "editor.glyphMargin": false,
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.wordWrap": "off",
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  // "jest.autoEnable": false,
  // "jest.enableInlineErrorMessages": false,
  // "prettier.eslintIntegration": true,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "window.zoomLevel": 0,
  "editor.quickSuggestionsDelay": 7,
  "files.autoGuessEncoding": false,
  "search.exclude": {
    "./priv/static/": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/_site": true,
    "**/tmp": true,
    "**/*.pyc": true,
    "**/*.asset-cache": true,
    "**/*.cache": true,
    "**/*.jekyll-cache": true,
    "**/*.tweet-cache": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "**/*.egg-info": true,
    "**/*.coverage": true,
    "**/celerybeat-schedule": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/vendor": true,
    "**/deps": true,
    "**/log": true,
    "**/public": true
  },
  "files.exclude": {
    "**/_build": true,
    "**/.elixir_ls": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/_site": true,
    "**/tmp": true,
    "**/*.pyc": true,
    "**/*.asset-cache": true,
    "**/*.cache": true,
    "**/*.jekyll-cache": true,
    "**/*.tweet-cache": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "**/*.egg-info": true,
    "**/*.coverage": true,
    "**/celerybeat-schedule": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/vendor": true,
    "**/deps": true,
    "**/log": true,
    "**/public": true
  },
  "files.trimFinalNewlines": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.fontAliasing": "antialiased",
  "workbench.statusBar.feedback.visible": false,
  "window.nativeTabs": false,
  "terminal.external.osxExec": "iTerm.app",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  //"jest.enableCodeLens": false,
  // "jest.enableSnapshotPreviews": false,
  "workbench.activityBar.visible": false,
  "workbench.sideBar.location": "left",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.colorTheme": "Monokai"
  // "elixirLS.suggestSpecs": false,
}
